<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PlayFragment">

    <fragment
        android:id="@+id/ux_fragment"
        android:name="nl.utwente.utgo.arcorelocation.NFSArFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/popup_button"
        android:layout_width="@dimen/popup_button_dimen"
        android:layout_height="@dimen/popup_button_dimen"
        android:layout_marginBottom="@dimen/scroll_padding_bottom"
        android:src="@drawable/ic_outline_keyboard_arrow_up_24"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/ux_fragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:tint="@color/text_color" />


</androidx.constraintlayout.widget.ConstraintLayout>